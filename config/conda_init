if [ $? -eq 0 ]; then
    eval "$__conda_setup"
else
    if [ -f "/dss/dsshome1/0C/ge87say4/miniforge3/etc/profile.d/conda.sh" ]; then
        . "/dss/dsshome1/0C/ge87say4/miniforge3/etc/profile.d/conda.sh"
    else
        export PATH="/dss/dsshome1/0C/ge87say4/miniforge3/bin:$PATH"
    fi
fi
unset __conda_setup

if [ -f "/dss/dsshome1/0C/ge87say4/miniforge3/etc/profile.d/mamba.sh" ]; then
